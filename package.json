{
  "name": "jeic-vue",
  "version": "1.0.2",
  "author": "huaiyong <770393167@qq.com>",
  "description": "Easy invoicing system stand-alone desktop version,An electron-vue sqlite3 project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/huaiyong/jeic-vue.git"
  },
  "keywords": [
    "electron",
    "electron-vue",
    "sqlite3"
  ],
  "scripts": {
    "build": "node --max_old_space_size=8192 .electron-vue/build.js && electron-builder",
    "build:ci": "node .electron-vue/build.js && electron-builder --publish always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "install-app-deps",
    "packageVersion": "echo $npm_package_version"
  },
  "build": {
    "productName": "jeic-vue",
    "copyright": "huaiyong",
    "appId": "org.huaiyong.jeic-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/logo.ico"
    },
    "win": {
      "icon": "build/icons/logo.ico",
      "extraResources": {
        "from": "./SunVoteSDK_x64.dll",
        "to": "./SunVoteSDK_x64.dll"
      }
    },
    "linux": {
      "icon": "build/icons/logo.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "gitHooks": {
    "commit-msg": "node verify_commit_msg.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "exec": "^0.2.1",
    "ffi": "^2.3.0",
    "node-uuid": "^1.4.8",
    "screenshot-desktop": "^1.9.0",
    "socket.io": "^2.2.0",
    "sqlite3": "^4.0.1",
    "uws": "^9.14.0",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-qr": "^2.1.0",
    "vue-socket.io": "^3.0.7",
    "vuedraggable": "^2.23.0"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "dayjs": "^1.7.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "echarts": "^4.2.1",
    "electron": "^2.0.4",
    "electron-builder": "^20.43.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.8.5",
    "electron-settings": "^3.2.0",
    "electron-updater": "^2.23.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "ffi": "^2.3.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.4.1",
    "js-md5": "^0.7.3",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.0",
    "log4js": "^4.5.1",
    "multispinner": "^0.2.1",
    "node-gyp": "^3.8.0",
    "node-loader": "^0.6.0",
    "node-uuid": "^1.4.8",
    "node-xlsx": "^0.12.1",
    "python": "^0.0.4",
    "socket.io": "^2.2.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "uws": "^9.14.0",
    "vue": "^2.3.3",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-electron": "^1.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-router": "^2.5.3",
    "vue-socket.io": "^3.0.7",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vuex": "^2.3.1",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-hot-middleware": "^2.18.2",
    "yorkie": "^1.0.3"
  },
  "__npminstall_done": false
}
